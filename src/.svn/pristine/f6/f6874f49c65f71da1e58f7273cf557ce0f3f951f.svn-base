
/*头部导航*/
.head {height: 40px;line-height: 40px;position: fixed;top: 0px;left: 0;width: 100%;background: #27befa;background:linear-gradient(to right,#27befa,#4aa9f3);background:-moz-linear-gradient(right,#27befa,#4aa9f3);background:-o-linear-gradient(right,#27befa,#4aa9f3);background:-webkit-linear-gradient(left,#27befa,#4aa9f3);z-index:9;}
.head .back {height: 100%;width: 55px;background: url(../../assets/images/nav_icon_back@2x.png) no-repeat 30% center;background-size: 20%;display: block;position: absolute;top: 0px;left: 0;font-size:0.8rem;text-align:right;}
.y-confirm-order-h1 {display: block;height: 100%;text-align: center;font-size: 18px;color: #fff;padding:0 60px;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
.head .mail{line-height:40px;text-align:center;color:#999;position:absolute;top:0;right:10px;}
.head .close {height: 100%;width: 55px;background: url(../../assets/images/nav_icon_close@2x.png) no-repeat center center;background-size: 30%;display: block;position: absolute;top: 0px;right: 0;}
.head .oltxt {padding: 0 13px;height: 100%;display: block;position: absolute;right: 0;top: 0;color:#fff;}
.head .osear {height: 100%;width: 55px;background: url(../../assets/images/nav_icon_search@2x.png) no-repeat center center;background-size: 35%;display: block;position: absolute;top: 0px;left: 0;font-size:0.8rem;text-align:right;}
.head .interval{height:100%;width:45px;display:block;background:url(../../assets/images/nav_icon_filtrate@2x.png) center center no-repeat;background-size:40%;position:absolute;top:0;right:0;}
.head .search{height:100%;width:55px;display:block;background:url(../../assets/images/nav_icon_search@2x.png) center center no-repeat;background-size:35%;position:absolute;top:0;right:0;}
.head .cfsearch{height:100%;width:45px;display:block;background:url(../../assets/images/nav_icon_search@2x.png) center center no-repeat;background-size:40%;position:absolute;top:0;right:45px;}

.main{padding-top:40px;}

/*底部固定菜单*/
.y-ycar-foot {height: 48px;background: #fff;bottom: 0;position: fixed;left: 0;right: 0;}
.y-ycar-foot a {width: 25%;display: block;height: 100%;float: left;position:relative;}
.y-ycar-foot a .icon {display: block;padding-top: 30px;height: 20px;line-height: 20px;color: #817d7d;font-size: 12px;text-align: center;position: relative;}
.y-ycar-foot .icon i {position: absolute;top: 8px;left: 50%;right: 0;display: block;width: 22px;height: 22px;margin-left: -11px;}
.y-ycar-foot a:nth-child(1) i {background: url(../images/tab_icon_home.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a:nth-child(2) i {background: url(../images/tab_icon_bill_default.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a:nth-child(3) i {background: url(../images/tab_icon_message_default.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a:nth-child(4) i {background: url(../images/tab_icon_me_default.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a.on .icon {color: #60b3f6;}
.y-ycar-foot a.on:nth-child(1) i {background:url(../images/tab_icon_home_selected.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a.on:nth-child(2) i {background:url(../images/tab_icon_bill_selected.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a.on:nth-child(3) i {background:url(../images/tab_icon_message_selected.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a.on:nth-child(4) i {background:url(../images/tab_icon_me_selected.png) center center no-repeat;background-size:contain;}


/*过渡动画*/
.vux-pop-out-enter-active,.vux-pop-out-leave-active,.vux-pop-in-enter-active,.vux-pop-in-leave-active {will-change: transform;transition: all 300ms;width: 100%;height:100%;top: 0px;position: absolute;backface-visibility: hidden;perspective: 1000;}
.vux-pop-out-enter {opacity: 0;transform: translate3d(-100%, 0, 0);}
.vux-pop-out-leave-active {opacity: 0;transform: translate3d(100%, 0, 0);}
.vux-pop-in-enter {opacity: 0;transform: translate3d(100%, 0, 0);}
.vux-pop-in-leave-active {opacity: 0;transform: translate3d(-100%, 0, 0);}

.touchleft{transform: translateX(-20%);-webkit-transform: translateX(-20%);-moz-transform: translateX(-20%);}
.touchright{transform: translateX(0%);-webkit-transform: translateX(0%);-moz-transform: translateX(0%);}
